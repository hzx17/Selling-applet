page{
  background:#f4f5f7;
}

.login_box{

  width: 100%;
  height:  700px ;
}
.login_logo{
  width: 100%;
  height: 100px;
}
.login_logo_box{
  width: 50%;
  height: 100px;
  margin: 0 auto;
}
.login_welcome{
  width: 100%;
  height: 30px;
  font-size: 18px;
  font-weight: bold;
  text-align: center;
  line-height: 30px;
  color: orange;
  /* background: chartreuse; */
}
.login_usernum{
  width: 90%;
    height: 40px;
  border-bottom: 1px solid #8a8a8a;
  margin: 0 auto;
  margin-top: 10px;
}
.usernum_input{
  width: 100%;
  height: 40px;
line-height: 40px;
}
.login_password{
  width: 90%;
    height: 40px;
  border-bottom: 1px solid #8a8a8a;
  margin: 0 auto;
  margin-top: 10px;
}
.password_input{
  width: 75%;
  height: 40px;
  line-height: 40px;
  float: left;
}
.login_password_huqu{
  width: 70px;
  height: 20px;
  font-size: 12px;
  line-height: 20px;
  border: 1px orange solid;
  border-radius: 10px;
  text-align: center;
  color: orange;
  float: left;
  margin-top: 9px;
}
.login_botton{
  width: 100%;
  height: 50px;
  margin-top: 10px;
}
.login_bottonlogin{
  width: 100px;
  height: 40px;
  background: orange;
  border-radius: 20px;
  color: white;
}
.login_relogin{
  width: 150px;
  height: 30px;
  border: 1px #8a8a8a solid;
  font-size: 14px;
  font-weight: bold;
  line-height: 30px;
  text-align: center;
  margin: 0 auto;
}
.login_icon{
  width: 200px;
  height: 40px;
  margin: 0 auto;
  margin-top: 10px;
}
.login_icon_weixin{
  width: 40px;
  height: 40px;
  padding-right: 40px;
  float: left;
}
.login_icon_QQ{
  width: 40px;
  height: 40px;
  padding-right: 40px;
  float: left;
}
.login_icon_zhifu{
  width: 40px;
  height: 40px;
  float: right;
}
.login_text{
  margin-top: 10px;
  width: 95%;
  padding-left: 10px;
  height: 40px;
  font-size: 16px;
  color: #8a8a8a;
}